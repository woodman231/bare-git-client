{
  "name": "@woodman231/bare-git-client",
  "version": "1.1.1",
  "description": "A TypeScript library for managing bare Git repositories with a clean, type-safe API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "git",
    "bare",
    "repository",
    "isomorphic-git",
    "typescript",
    "version-control"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "isomorphic-git": "^1.36.1",
    "zod": "^3.24.1"
  }
}
